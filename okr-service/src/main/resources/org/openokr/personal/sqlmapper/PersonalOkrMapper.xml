<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.okr.personal.sqlmapper.PersonalOkrMapper">

    <select id="getMenuByName" parameterType="java.util.Map" resultType="org.openokr.ssm.vo.MenuVO">
        select t1.*
        from t_pfm_ssm_menu t1
        where t1.name = #{name}
    </select>

</mapper>